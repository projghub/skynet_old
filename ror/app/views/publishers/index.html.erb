<h1>Listing Publishers</h1>

<table class="list">
	<thead>
		<tr>
			<th>Name</th>
			<th>Website</th>
			<th>Phone</th>
			<th>Contact Name</th>
			<th>Access Hash</th>
			<th>Enabled?</th>
			<th class="action"></th>
		</tr>
	</thead>

	<tbody>
		<% @publishers.each do |publisher| %>
			<tr>
				<td><%= publisher.name %></td>
				<td><%= publisher.website %></td>
				<td><%= publisher.phone %></td>
				<td><%= publisher.contact_name %></td>
				<td><%= publisher.access_hash %></td>
				<td><%= publisher.enabled ? "Yes" : "No" %></td>
				<td class="actions">
					<%= link_to 'Show', publisher %>
					<%= link_to 'Edit', edit_publisher_path(publisher) %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>

<%= link_to 'New Publisher', new_publisher_path %>
