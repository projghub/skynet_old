<h1>Listing Users</h1>

<table>
	<thead>
		<tr>
			<th>Username</th>
			<th>Email</th>
			<th>Enabled?</th>
			<th></th>
			<th></th>
		</tr>
	</thead>

	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= user.username %></td>
				<td><%= user.email %></td>
				<td><%= user.enabled ? "Yes" : "No" %></td>
				<td><%= link_to 'Show', user %></td>
				<td><%= link_to 'Edit', edit_user_path(user) %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<br />

<%= link_to 'New User', new_user_path %>
