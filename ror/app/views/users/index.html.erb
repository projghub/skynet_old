<h1>Listing users</h1>

<table>
  <tr>
    <th>Account</th>
    <th>Username</th>
    <th>Email</th>
    <th>Enabled?</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.account.name %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.enabled ? "Yes" : "No" %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
